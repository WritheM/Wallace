      "skip": {
        "reasons": {
          "theme": "This song does not fit the room theme. Check http://wiki.writhem.com/display/radio/Our+Music+Choices for more information.",
          "op": "second sample"
        },
        "permission": ["BOUNCER"]
      }
    },
    "dependencies": ["plug"]
  }
}
